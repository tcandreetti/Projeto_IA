        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Double_peg_box"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Straight_peg_box"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Double_head -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Double_head">
