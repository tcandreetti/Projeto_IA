        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Other_material"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Euphonium -->

