    <!-- http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Cord -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Cord">
