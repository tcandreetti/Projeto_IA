    <!-- http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Inventionshorn -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Inventionshorn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#French_horns"/>
        <rdfs:comment rdf:datatype="&xsd;language">This is crooked in the tuning slide; all horns with fixed mouthpipes, as distinct from crooks, are included here. The natural french horns will be similarly distinguished</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Ivory -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2011/3/Jeremy_Montagu.owl#Ivory">
